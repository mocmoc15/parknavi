<h1>トップページ</h1>

  <% if user_signed_in? %>
   <li><%= link_to current_user.nickname, "/users/#{current_user.id}"  %></li>
   <li><%= link_to 'ログアウト', destroy_user_session_path, method: :delete %></li>
  <% else %>
   <li><%= link_to 'ログイン', new_user_session_path %></li>
   <li><%= link_to '新規登録', new_user_registration_path %></li>
  <% end %>
 

<h3><%= link_to 'キーワードから探す', search_parks_path %></h3>
<h3><%= link_to '地図から探す', "#" %></h3>


<% @parks.each do |park| %>
  <li class='list'>
    <%= link_to park_path(park.id), method: :get do %>
       <%# <%= image_tag park.images[0] %>
      <%= park.name %><br>
      <%= park.prefecture[:name] %><br> 
    <% end %> 
  </li>
<% end %> 